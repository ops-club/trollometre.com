{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["svelte3"],
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [
        {
            "files": ["*.svelte"],
            "processor": "svelte3/svelte3"
        }
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    }
}
